{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "大师拷贝",
  "version": "3.0.4",
  "identifier": "com.redwinam.mascopy",
  "build": {
    "frontendDist": "../src-ui/dist",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "macOSPrivateApi": true,
    "windows": [
      {
        "title": "",
        "width": 1400,
        "height": 900,
        "resizable": true,
        "fullscreen": false,
        "decorations": true,
        "transparent": true,
        "titleBarStyle": "Overlay"
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "../icon.icns"
    ],
    "macOS": {
      "signingIdentity": "-"
    }
  }
}
